    "cypress": "^12.12.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "2.8.8",
    "typescript": "^5.0.2",
    "vite": "^4.3.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
